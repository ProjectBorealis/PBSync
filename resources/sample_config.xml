<?xml version="1.0" encoding="UTF-8"?>
<config>
    <project>
        <enginebaseversion>5.0</enginebaseversion>
        <uprojectname>SampleProject.uproject</uprojectname>
        <defaultgameinipath>Config/DefaultGame.ini</defaultgameinipath>
        <userconfig>.user-sync</userconfig>
        <ciconfig>.ci-sync</ciconfig>
        <packagepdbs>True</packagepdbs>
    </project>
    <git>
        <version>2.37.1.vfs.0.1</version>
        <lfsversion>3.2.0</lfsversion>
        <gcmversion>2.0.785</gcmversion>
        <expectedbranch>promoted</expectedbranch>
        <url>https://github.com/user/repo.git</url>
        <branches>
            <branch>main</branch>
            <branch>promoted</branch>
        </branches>
    </git>
    <versionator>
        <defaultbundle>editor</defaultbundle>
        <cibundle>ci-engine</cibundle>
        <engineprefix>SP</engineprefix>
        <enginetype>ue5</enginetype>
        <uses_gcs>True</uses_gcs>
    </versionator>
    <log>
        <file>pbsync_log.txt</file>
    </log>
    <publish>
        <publisher>butler</publisher>
        <stagedir>Saved/StagedBuilds</stagedir>
    </publish>
    <butler>
        <project>user/project</project>
        <manifest>Butler/.itch.{0}.toml</manifest>
    </butler>
    <dispatch>
        <config>Dispatch/DispatchConfig.json</config>
    </dispatch>
    <resharper>
        <version>2022.2.3</version>
    </resharper>
    <msg>
        <git_instructions>https://github.com/ProjectBorealis/PBCore/wiki/Prerequisites</git_instructions>
        <support_channel>the support channel</support_channel>
    </msg>
</config>

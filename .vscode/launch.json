{
    "version": "0.2.0",
    "inputs": [
        {
            "id": "debugPath",
            "description": "Please enter working directory for PBSync",
            "default": "C:\\pb",
            "type": "promptString"
        },
        {
            "id": "debugBranch",
            "description": "Please enter branch to be used as expected branch",
            "default": "main",
            "type": "promptString"
        },
        {
            "id": "dispatchPath",
            "description": "Please enter path for dispatch executable",
            "default": "",
            "type": "promptString"
        },
        {
            "id": "printSelection",
            "description": "Please choose what do you want to print",
            "default": "current-engine",
            "options": ["current-engine", "project"],
            "type": "pickString"
        },
        {
            "id": "autoversionSelection",
            "description": "Please choose version increase type",
            "default": "hotfix",
            "options": ["hotfix", "update", "release"],
            "type": "pickString"
        },
        {
            "id": "guiSelection",
            "description": "Please choose GUI page",
            "default": "sync",
            "options": [
                "sync",
                "settings"
            ],
            "type": "pickString"
        },
        {
            "id": "publishSelection",
            "description": "Please choose build type to publish",
            "default": "internal",
            "options": ["internal", "playtester"],
            "type": "pickString"
        },
        {
            "id": "bundleSelection",
            "description": "Please choose what engine bundle you want to use",
            "default": "editor",
            "options": ["editor", "ciengine"],
            "type": "pickString"
        },
        {
            "id": "commit",
            "description": "Commit contains moves to migrate assets using",
            "type": "promptString"
        },
        {
            "id": "glob",
            "description": "Glob filter to choose assets being migrated",
            "default": "*.uasset",
            "type": "promptString"
        }
    ],
    "configurations": [
        {
            "name": "Print help",
            "args": ["--help"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Sync All",
            "args": ["--sync", "all", "--debugpath", "${input:debugPath}", "--debugbranch", "${input:debugBranch}"],
            "type": "python",
            "justMyCode": false,
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "GUI",
            "args": [
                "--gui",
                "${input:guiSelection}",
                "--debugpath",
                "${input:debugPath}"
            ],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Pull Binaries",
            "args": ["--sync", "binaries", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Pull Engine Build",
            "args": ["--sync", "engine", "--bundle", "${input:bundleSelection}", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "justMyCode": false,
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Generate DDC",
            "args": ["--sync", "ddc", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Build Source",
            "args": ["--build", "source", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Build Release",
            "args": ["--build", "release", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Inspect Code",
            "args": ["--build", "inspect", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Update Engine Version to Latest",
            "args": ["--sync", "engineversion", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Print Latest Engine Version",
            "args": ["--printversion", "latest-engine", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Print Information",
            "args": ["--printversion", "${input:printSelection}", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Project Version Update",
            "args": ["--autoversion", "${input:autoversionSelection}", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Publish Dispatch Build",
            "args": ["--publish", "${input:publishSelection}", "--dispatch", "${input:dispatchPath}", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        },
        {
            "name": "Migrate Assets",
            "args": ["--migrate", "${input:commit}", "--migrate_glob", "${input:glob}", "--debugpath", "${input:debugPath}"],
            "type": "python",
            "request": "launch",
            "module": "pbsync",
            "console": "integratedTerminal"
        }
    ]
}
